{
  "name": "coffee-platform-rbac",
  "version": "1.0.0",
  "description": "Role-Based Access Control framework for Coffee Platform",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "bunx biome check .",
    "typecheck": "bunx tsc --noEmit"
  },
  "keywords": [
    "rbac",
    "access-control",
    "permissions",
    "coffee-platform"
  ],
  "author": "CertilinkAdmin <cole@certilink.tech>",
  "license": "MIT",
  "dependencies": {
    "xml2js": "^0.6.2",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/xml2js": "^0.4.14",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.45",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "@testing-library/react": "^14.1.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
